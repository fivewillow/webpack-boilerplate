{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "A webpack boilerplate",
  "main": "index.js",
  "scripts": {
    "prod": "env-cmd -f ./environments/dev.env.json webpack --mode production  --config config/webpack.prod.config.babel.js",
    "dev": "env-cmd -f ./environments/dev.env.json webpack --mode development  --config config/webpack.dev.config.babel.js",
    "serve": "env-cmd -f ./environments/dev.env.json webpack serve --mode development  --config config/webpack.dev.config.babel.js",
    "lint": "eslint . --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Lawrence Lin",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@tsconfig/recommended": "^1.0.1",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "env-cmd": "^10.1.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-webpack-plugin": "^2.5.4",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.6.0",
    "postcss-inline-svg": "^5.0.0",
    "postcss-loader": "^6.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.3.1",
    "prettier-webpack-plugin": "^1.2.0",
    "style-loader": "^2.0.0",
    "typescript": "^4.3.2",
    "webpack": "^5.39.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^4.0.0-beta.3",
    "webpack-merge": "^5.8.0"
