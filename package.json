{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "A webpack boilerplate",
  "main": "index.js",
  "scripts": {
    "prod": "env-cmd -f ./environments/dev.env.json webpack --mode production  --config config/webpack.prod.config.babel.js",
    "dev": "env-cmd -f ./environments/dev.env.json webpack --mode development  --config config/webpack.dev.config.babel.js",
    "serve": "env-cmd -f ./environments/dev.env.json webpack serve --mode development  --config config/webpack.dev.config.babel.js",
    "lint": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Lawrence Lin",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@tsconfig/recommended": "^1.0.1",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-webpack-plugin": "^3.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss-inline-svg": "^5.0.0",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "prettier-webpack-plugin": "^1.2.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {}
}
